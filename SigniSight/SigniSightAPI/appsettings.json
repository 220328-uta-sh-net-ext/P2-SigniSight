{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Error"
      }
      
    }
  },
  "WriteTo": [
    {
      "Name": "MSSqlServer",
      "Args": {
        "connectionString": "../SigniSightDL/connection-string.txt",
        "tableName": "Logs",
        "autoCreateSqlTable": true
      } 
    }
  ]
}
